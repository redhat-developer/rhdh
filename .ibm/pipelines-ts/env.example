# Red Hat Developer Hub Pipeline Configuration

# Kubernetes Configuration
K8S_CLUSTER_URL=https://api.cluster.example.com:6443
K8S_CLUSTER_TOKEN=your-cluster-token
K8S_CLUSTER_ROUTER_BASE=apps.cluster.example.com
KUBECONFIG=/path/to/kubeconfig

# Container Registry
QUAY_REPO=quay.io/rhdh/rhdh-hub
TAG_NAME=latest
CHART_VERSION=2.13.4

# Namespaces
NAMESPACE=showcase
NAMESPACE_RBAC=showcase-rbac

# Database Configuration
POSTGRES_PASSWORD=admin123
# For RDS:
# RDS_ENDPOINT=your-rds-endpoint.rds.amazonaws.com
# RDS_SSL_CERT_PATH=/path/to/rds-ca-bundle.pem

# Authentication Providers
# GitHub
GITHUB_APP_CLIENT_ID=your-github-client-id
GITHUB_APP_CLIENT_SECRET=your-github-client-secret

# Google
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Keycloak
KEYCLOAK_AUTH_BASE_URL=https://keycloak.example.com
KEYCLOAK_AUTH_REALM=backstage
KEYCLOAK_AUTH_CLIENT_ID=backstage
KEYCLOAK_AUTH_CLIENT_SECRET=your-keycloak-secret

# OIDC
OIDC_ISSUER=https://oidc.example.com
OIDC_CLIENT_ID=backstage
OIDC_CLIENT_SECRET=your-oidc-secret

# Backstage Configuration
BACKEND_SECRET=your-backend-secret

# Testing
PLAYWRIGHT_WORKERS=4
TEST_TIMEOUT=600000

# Monitoring
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
REPORT_PORTAL_URL=https://reportportal.example.com

# Cloud Provider Specific
# AKS
ARM_CLIENT_ID=your-azure-client-id
ARM_CLIENT_SECRET=your-azure-client-secret
ARM_SUBSCRIPTION_ID=your-azure-subscription-id
ARM_TENANT_ID=your-azure-tenant-id
AKS_CLUSTER_NAME=your-aks-cluster
AKS_RESOURCE_GROUP=your-resource-group

# GKE
GOOGLE_CLOUD_PROJECT=your-gcp-project
GKE_CLUSTER_NAME=your-gke-cluster
GKE_CLUSTER_REGION=us-central1

# Job Configuration
JOB_NAME=ocp-nightly
DEPLOYMENT_ID=1

# Feature Flags
ENABLE_MONITORING=true
ENABLE_RBAC=false
ENABLE_REDIS_CACHE=true
ENABLE_DYNAMIC_PLUGINS=true
DRY_RUN=false 